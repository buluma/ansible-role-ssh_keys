---
#
# Ansible managed
#
exclude_paths:
  - meta/preferences.yml
  - molecule/default/prepare.yml
  - molecule/default/converge.yml
  - molecule/default/verify.yml
  - molecule/default/collections.yml
  - .tox
  - .cache
  - .github
  - requirements.yml

skip_list:
  - yaml[truthy]

enable_list:
  - name[prefix]

warn_list:
  - role-name
  - name[play]
  - name[casing]
